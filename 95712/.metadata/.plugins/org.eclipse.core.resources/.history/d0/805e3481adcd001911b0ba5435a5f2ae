		int i = 16777216; //2^24
		float f = i;
		System.out.println("i = " +  i + " f = " + f);
		i++;
		f = i;
		System.out.println("i = " +  i + " f = " + f);
		i++;
		f = i;
		System.out.println("i = " +  i + " f = " + f);
		i++;
		f = i;
		System.out.println("i = " +  i + " f = " + f);

	}

